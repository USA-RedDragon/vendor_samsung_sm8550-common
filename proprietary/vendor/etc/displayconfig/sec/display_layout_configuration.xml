<!-- samsung device uses this path -->
<!-- vendor/etc/displayconfig/ -->
<layouts>
  <!-- CLOSE -->
  <layout>
    <state>0</state>
    <display defaultDisplay="false" enabled="false">
      <address>4630947175568309891</address>
    </display>
    <display defaultDisplay="true" enabled="true">
      <address>4630946481096930692</address>
    </display>
  </layout>

  <!-- TENT -->
  <layout>
    <state>1</state>
    <display defaultDisplay="false" enabled="false">
      <address>4630947175568309891</address>
    </display>
    <display defaultDisplay="true" enabled="true">
      <address>4630946481096930692</address>
    </display>
  </layout>

  <!-- HALF FOLD -->
  <layout>
    <state>2</state>
    <display defaultDisplay="true" enabled="true">
      <address>4630947175568309891</address>
    </display>
    <display defaultDisplay="false" enabled="false">
      <address>4630946481096930692</address>
    </display>
  </layout>

  <!-- OPEN -->
  <layout>
    <state>3</state>
    <display defaultDisplay="true" enabled="true">
      <address>4630947175568309891</address>
    </display>
    <display defaultDisplay="false" enabled="false">
      <address>4630946481096930692</address>
    </display>
  </layout>

  <!-- DUAL -->
  <layout>
    <state>4</state>
    <display defaultDisplay="true" enabled="true">
      <address>4630947175568309891</address>
      <position>front</position>
    </display>
    <display defaultDisplay="false" enabled="true">
      <address>4630946481096930692</address>
      <position>rear</position>
    </display>
  </layout>

  <!-- REAR DUAL -->
  <layout>
    <state>5</state>
    <display defaultDisplay="false" enabled="true">
      <address>4630947175568309891</address>
      <position>rear</position>
    </display>
    <display defaultDisplay="true" enabled="true">
      <address>4630946481096930692</address>
      <position>front</position>
    </display>
  </layout>
</layouts>
